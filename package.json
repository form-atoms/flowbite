{
  "name": "@form-atoms/flowbite",
  "version": "0.0.0-semantic-release",
  "author": "Miroslav Petrik <miroslavpetrik@outlook.com>",
  "homepage": "https://github.com/form-atoms/flowbite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/form-atoms/flowbite.git"
  },
  "bugs": "https://github.com/form-atoms/flowbite/issues",
  "license": "MIT",
  "keywords": [
    "react atom form",
    "react field",
    "react input",
    "flowbite react",
    "flowbite form field",
    "form atoms",
    "form atoms field",
    "jotai",
    "jotai form",
    "react",
    "react form",
    "react hook"
  ],
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "typecheck": "vitest --typecheck.only --passWithNoTests",
    "publish": "yarn npm publish --access public",
    "build": "tsc --build ./tsconfig.build.json",
    "flowbite:build": "flowbite-react build",
    "storybook": "storybook dev",
    "storybook:build": "storybook build",
    "storybook:uprade": "yarn dlx storybook@latest upgrade",
    "semantic-release": "semantic-release",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint --ext .tsx,.ts ./src",
    "lint:fix": "yarn run lint --fix",
    "upgrade-packages": "yarn upgrade-interactive",
    "upgrade-yarn": "yarn set version stable && yarn install",
    "postinstall": "flowbite-react register"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@form-atoms/field": "^6.2.0",
    "@form-atoms/list-atom": "^2.3.13",
    "@mdx-js/react": "^3.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook/addon-docs": "^10.0.1",
    "@storybook/addon-links": "^10.0.1",
    "@storybook/react-vite": "^10.0.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "10.0.1",
    "flowbite-react": "^0.12.10",
    "form-atoms": "3.3.3",
    "happy-dom": "20.0.8",
    "jotai": "^2.15.0",
    "jotai-devtools": "0.8.0",
    "jotai-effect": "^2.1.3",
    "postcss": "8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "semantic-release": "^25.0.1",
    "storybook": "^10.0.1",
    "tailwindcss": "^4.1.13",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.11",
    "vitest": "4.0.4",
    "zod": "^4.1.12"
  },
  "dependencies": {
    "react-render-prop-type": "0.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "peerDependencies": {
    "@form-atoms/field": "^5",
    "flowbite-react": "^0.12",
    "form-atoms": "^3",
    "jotai": "^2",
    "react": ">=16.8"
  },
  "packageManager": "yarn@4.10.3"
}
