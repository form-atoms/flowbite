{
  "name": "@form-atoms/flowbite",
  "version": "0.0.0-semantic-release",
  "author": "Miroslav Petrik <miroslavpetrik@outlook.com>",
  "homepage": "https://github.com/form-atoms/flowbite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/form-atoms/flowbite.git"
  },
  "bugs": "https://github.com/form-atoms/flowbite/issues",
  "license": "MIT",
  "keywords": [
    "react atom form",
    "react field",
    "react input",
    "flowbite react",
    "flowbite form field",
    "form atoms",
    "form atoms field",
    "jotai",
    "jotai form",
    "react",
    "react form",
    "react hook"
  ],
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "typecheck": "vitest --typecheck.only --passWithNoTests",
    "publish": "yarn npm publish --access public",
    "build": "tsc --build ./tsconfig.build.json",
    "sb": "yarn storybook",
    "sb:build": "storybook build",
    "sb:up": "yarn dlx storybook@latest upgrade",
    "storybook": "storybook dev",
    "semantic-release": "semantic-release",
    "lint": "eslint --ext .tsx,.ts ./src",
    "lint:fix": "yarn run lint --fix",
    "ui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@form-atoms/field": "^5.1.0",
    "@form-atoms/list-atom": "^1.0.11",
    "@mdx-js/react": "^2.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.4",
    "@semantic-release/npm": "^11.0.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-docs": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/manager-api": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/theming": "^8.0.0",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/semantic-release": "^20.0.6",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "10.4.16",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "flowbite-react": "^0.7.3",
    "form-atoms": "3.2.4",
    "happy-dom": "12.10.3",
    "jotai": "^2.7.0",
    "jotai-devtools": "0.8.0",
    "jotai-effect": "^0.6.0",
    "postcss": "8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "semantic-release": "^22.0.8",
    "storybook": "^8.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "5.3.3",
    "vite": "^5.0.11",
    "vitest": "1.1.3",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@storybook/test": "8.0.0",
    "react-render-prop-type": "0.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "peerDependencies": {
    "@form-atoms/field": "^5",
    "flowbite-react": "^0.7.0",
    "form-atoms": "^3",
    "jotai": "^2",
    "react": ">=16.8"
  },
  "packageManager": "yarn@4.0.2"
}
