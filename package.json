{
  "name": "@form-atoms/flowbite",
  "version": "0.0.0-semantic-release",
  "author": "Miroslav Petrik <miroslavpetrik@outlook.com>",
  "homepage": "https://github.com/form-atoms/flowbite#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/form-atoms/flowbite.git"
  },
  "bugs": "https://github.com/form-atoms/flowbite/issues",
  "license": "MIT",
  "keywords": [
    "react atom form",
    "react field",
    "react input",
    "flowbite react",
    "flowbite form field",
    "form atoms",
    "form atoms field",
    "jotai",
    "jotai form",
    "react",
    "react form",
    "react hook"
  ],
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "typecheck": "vitest --typecheck.only --passWithNoTests",
    "publish": "yarn npm publish --access public",
    "build": "tsc --build ./tsconfig.build.json",
    "storybook:build": "storybook build",
    "storybook:uprade": "yarn dlx storybook@latest upgrade",
    "storybook": "storybook dev",
    "semantic-release": "semantic-release",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint --ext .tsx,.ts ./src",
    "lint:fix": "yarn run lint --fix",
    "upgrade-packages": "yarn upgrade-interactive",
    "postinstall": "flowbite-react register"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@form-atoms/field": "^5.1.2",
    "@form-atoms/list-atom": "^1.0.12",
    "@mdx-js/react": "^3.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.2",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/addon-links": "^9.1.6",
    "@storybook/react-vite": "^9.1.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/semantic-release": "^20.0.6",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "10.4.21",
    "eslint": "8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-storybook": "9.1.6",
    "flowbite-react": "^0.12.9",
    "form-atoms": "3.3.1",
    "happy-dom": "12.10.3",
    "jotai": "^2.7.1",
    "jotai-devtools": "0.8.0",
    "jotai-effect": "^0.6.0",
    "postcss": "8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "semantic-release": "^23.0.6",
    "storybook": "^9.1.6",
    "tailwindcss": "^3.4.1",
    "typescript": "5.3.3",
    "vite": "^7.1.6",
    "vitest": "3.2.4",
    "zod": "^3.25.76"
  },
  "dependencies": {
    "react-render-prop-type": "0.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "peerDependencies": {
    "@form-atoms/field": "^5",
    "flowbite-react": "^0.10",
    "form-atoms": "^3",
    "jotai": "^2",
    "react": ">=16.8"
  },
  "packageManager": "yarn@4.0.2"
}
